<Window x:Class="IPCameraSettings.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Camera API - Settings" Height="450" Width="400">
    <Grid Margin="10">
        <StackPanel>
            <TextBlock Text="Stream Settings" Margin="0,5,0,5"/>
            <TextBox Name="SettingsBox" IsReadOnly="True" Height="200" VerticalScrollBarVisibility="Auto"/>

            
        </StackPanel>
    </Grid>
</Window>